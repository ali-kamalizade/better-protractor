{
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"baseUrl": "",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2016",
			"dom"
		],
		"module": "commonjs",
		"removeComments": false,
		"noImplicitAny": false
	},
	"exclude": [
		"node_modules"
	]
}
